		<td width="50" valign="top"><span class="time">%time%: </span></td>
		<td xwidth="100%">%text%</td>
	</tr>
</table>

<!--hStatus-->
<div>
	<span class="status history time">%date% at %time%</span><br>
	<span class="status history name">%name%</span><br>
	<span class="status history message">%text%</span>
</div>

<!--hURL-->
<div>
	<span class="status history name">%name%</span><br>
	<span class="status history time">(%time%):</span><br>
	<span class="status history message">URL: %text%</span>
</div>

<!--hFile-->
<div>
	<span class="status history name">%name%</span><br>
	<span class="status history time">(%time%):</span><br>
	<span class="status history message">File: %text%</span>
</div>


