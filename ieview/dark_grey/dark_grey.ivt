<!--HTMLStart-->
<html>

<head>
	<base href="%base%">
	<link rel="stylesheet" type="text/css" href="dark-grey.css" />
</head>

<body>

<!--MessageOut-->
<table width="100%" class="msg_out head" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td width="16" class="bullet" valign="top"><img src="msg_out.png" /></td>
		<td xwidth="100%"><span class="name">%name%, </span><span class="date">%date%</span></td>
	</tr>
</table>
<table width="100%" class="msg_out" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td width="16" class="bullet"></td>
		<td width="50" valign="top"><span class="time">%time%: </span></td>
		<td xwidth="100%">%text%</td>
	</tr>
</table>

<!--MessageIn-->
<table width="100%" class="msg_in head" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td width="16" class="bullet" valign="top"><img src="msg_in.png" /></td>
		<td xwidth="100%"><span class="name">%name%, </span><span class="date">%date%</span></td>
	</tr>
</table>
<table width="100%" class="msg_in" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td width="16" class="bullet"></td>
		<td width="50" valign="top"><span class="time">%time%: </span></td>
		<td xwidth="100%">%text%</td>
	</tr>
</table>

<!--Status-->
<div>
	<span class="status time">%time% </span><br>
	<span class="status name">%name%</span><br>
	<span class="status message">%text%</span>
</div>

<!--URL-->
<div>
	<span class="status name">%name%</span><br>
	<span class="status time">%time%:</span><br>
	<span class="status message">URL: %text%</span>
</div>

<!--File-->
<div>
	<span class="status name">%name%</span><br>
	<span class="status time">%time%:</span><br>
	<span class="status message">File: %text%</span>
</div>

